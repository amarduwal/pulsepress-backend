{
  "name": "pulsepress-backend",
  "version": "1.0.0",
  "description": "PulsePress News Portal Backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch --inspect src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:worker": "node dist/jobs/workers.js",
    "migrate": "node -r tsx/cjs src/db/migrate.ts",
    "migrate:rollback": "tsx src/db/migrate.ts rollback",
    "migrate:reset": "tsx src/db/migrate.ts reset",
    "migrate:status": "tsx src/db/migrate.ts status",
    "migrate:fresh": "tsx src/db/migrate.ts fresh",
    "db:seed": "tsx src/db/migrate.ts seed",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "news",
    "aggregator",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mozilla/readability": "^0.5.0",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.1.5",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "compromise": "^14.11.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "isomorphic-dompurify": "^2.4.0",
    "jsdom": "^23.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.10.4",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-http": "^9.0.0",
    "pino-pretty": "^13.1.2",
    "redis": "^4.6.12",
    "rss-parser": "^3.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
